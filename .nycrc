{
  "exclude": [
    "app/test/",
    "node_modules/"
  ],
  "report-dir": "app/coverage",
  "check-coverage": false,
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "watermarks": {
    "lines": [100, 100],
    "statements": [100, 100],
    "functions": [100, 100],
    "branches": [100, 100]
  },
  "reporter": [
    "lcov"
  ],
  "require": [
    "babel-register",
    "babel-polyfill"
  ],
  "sourceMap": false,
  "instrument": false
}
